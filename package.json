{
	"name": "aibotpro-cli",
	"version": "0.0.11",
	"description": "Intelligent Command Line Assistant powered by AI",
	"license": "MIT",
	"bin": {
		"aibotpro": "dist/cli.js"
	},
	"type": "module",
	"keywords": [
		"cli",
		"ai",
		"assistant",
		"bot",
		"terminal"
	],
	"author": "Mufasa",
	"repository": {
		"type": "git",
		"url": "https://github.com/MayDay-wpf/aibotpro-cli.git"
	},
	"bugs": {
		"url": "https://github.com/MayDay-wpf/aibotpro-cli/issues"
	},
	"homepage": "https://github.com/MayDay-wpf/aibotpro-cli#readme",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/cli.js",
		"prepublishOnly": "npm run build",
		"test": "prettier --check . && xo && ava",
		"lint": "xo",
		"format": "prettier --write ."
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"figlet": "^1.8.2",
		"ink": "^4.1.0",
		"ink-text-input": "^5.0.1",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/figlet": "^1.7.0",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
